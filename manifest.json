{
    "manifest_version": 3,
    "name": "LeetCode Focus",
    "version": "1.0",
    "description": "Hides the LeetCode home page to help you stay focused.",
    "content_scripts": [
      {
        "matches": ["*://leetcode.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["scripting"]
  }
  